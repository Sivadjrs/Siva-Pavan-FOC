#include <stdio.h>

int main() {

    int n;

    printf("Enter the value of n: ");

    scanf("%d", &n);

    int sum = 0;

    for (int i = 2; i <= n; i++) {

        sum = sum + (i*i);

    }

    printf("The sum of squares of the consecutive natural numbers from 2 to %d is %d\n", n, sum);

    return 0;

}
