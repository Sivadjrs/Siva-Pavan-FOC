#include <stdio.h>

int main() {

    int start, end;

    printf("Enter starting number: ");

    scanf("%d", &start);

    printf("Enter ending number: ");

    scanf("%d", &end);

    if (start % 2 == 0)

    {

    start++; // If start is even, increment it to make it odd

    }

    for (int i = start; i <= end; i += 2) {

    printf("%d ", i);

    }

    return 0;

}
